	<queryString>
		<![CDATA[SELECT PRODUCT_ID, PRODUCT_NAME, GROUP_NAME, PRICE, CREATED_DATE FROM PRODUCT WHERE PRICE > 150.00]]>
	</queryString>


	<field name="PRODUCT_ID" class="java.lang.Integer"/>
	<field name="PRODUCT_NAME" class="java.lang.String"/>
	<field name="GROUP_NAME" class="java.lang.String"/>
	<field name="PRICE" class="java.lang.Float"/>

	<variable name="FULL_PRODUCT_NAME" class="java.lang.String" >
		<variableExpression><![CDATA[ $F{GROUP_NAME} + "/" + $F{PRODUCT_NAME} ]]></variableExpression>
	</variable>

